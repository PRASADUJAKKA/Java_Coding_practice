import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      
      int num = 12655579;
      int rem = 0;
      int cnt = 0;
      
      while(num>0){
        rem = num%10; 
        num /= 10; 
        cnt++;
      }
      System.out.println(cnt);
    }
}
