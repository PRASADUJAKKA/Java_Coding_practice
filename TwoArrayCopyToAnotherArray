import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
 
      int[] i = {1,2,3,4};
      int[] j = {5,6,7,8};
      
      int m = i.length;
      int n = j.length;
      int o = m + n;
      
      int[] k = new int[o];
      
      System.arraycopy(i,0,k,0,m);
      System.arraycopy(j,0,k,m,n);
      
      System.out.println(Arrays.toString(k));
 
  }
}
