import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      
      int num1 = 4;
      int num2 = 18;
      int gcd = 1;
      
      for(int i=1;i<num2;i++){
        if(num1%i == 0 && num2%i == 0){
          if(i>=gcd){
            gcd = i;
          }
        }
      }
      System.out.println("GCD of Two numbers "+num1+" & "+num2+" is "+gcd);
    }
}
