public class JavaP{
public static void main(String[] args){
      int[] arr1 = {2,7,11,15,-2};
      int target = 9;
      for(int i=0;i<arr1.length;i++)
      {
        for(int j=0;j<arr1.length;j++){
          if(arr1[i] + arr1[j] == target){
            System.out.println(arr1[i]+ "  "+ arr1[j] );
          }
        }
      }
  }     
}
